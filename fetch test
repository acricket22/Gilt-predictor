import pdblp
from datetime import datetime, date

con = pdblp.BCon(host="localhost", port=8194, timeout=5000)
con.start()

start = date(2025, 11, 1)
end   = date(2025, 11, 15)

print("Trying SPX Index...")
print(con.bdh("SPX Index", "PX_LAST", start, end))

print("Trying GT10 Govt...")
print(con.bdh("GT10 Govt", "PX_LAST", start, end))

con.stop()
